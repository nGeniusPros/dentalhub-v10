# Frontend Environment Variables
# These are exposed to the browser with VITE_ prefix
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Firebase
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_firebase_auth_domain
VITE_FIREBASE_PROJECT_ID=your_firebase_project_id

# NexHealth
VITE_NEXHEALTH_API_KEY=your_nexhealth_api_key
VITE_NEXHEALTH_PRACTICE_ID=your_nexhealth_practice_id

# Backend Environment Variables (Netlify Functions)
# These are NOT exposed to the browser

# Supabase Service Role (only used in Netlify Functions)
SUPABASE_URL=your_supabase_url
SUPABASE_SERVICE_KEY=your_supabase_service_role_key

# RetellAI (only used in Netlify Functions)
RETELL_API_KEY=your_retell_api_key
RETELL_AGENT_ID=your_retell_agent_id
RETELL_PHONE_NUMBER=your_retell_phone_number